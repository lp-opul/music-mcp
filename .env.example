# Ditto Music Distribution API Credentials
DITTO_EMAIL=your-email
DITTO_PASSWORD=your-password

# API URLs (defaults shown - usually don't need to change)
DITTO_BASE_URL=https://dashboard2.qa.dittomusic.com
DITTO_RELEASES_URL=https://releases.qa.dittomusic.com
DITTO_SALES_URL=https://sales.dittomusic.com
DITTO_TRENDS_URL=https://trends.dittomusic.com

# Basic Auth for QA environment access
DITTO_BASIC_USER=ditto
DITTO_BASIC_PASS=your-basic-pass

# Suno AI Music Generation
SUNO_API_KEY=your-suno-api-key

# x402 Payment Configuration
PAYMENT_WALLET_ADDRESS=0xYourWalletAddress

# Privy Wallet Configuration (for artist royalty wallets)
PRIVY_APP_ID=your_privy_app_id
PRIVY_APP_SECRET=your_privy_app_secret

# Upstash Redis for persistent rate limiting (optional - falls back to in-memory)
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# API Key Authentication (optional - set to true to require keys)
REQUIRE_API_KEY=false
# Format: key1:userId1:name1,key2:userId2:name2
API_KEYS=mcp_demo123:demo_user:Demo Key
